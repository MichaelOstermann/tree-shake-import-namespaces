{
    "name": "@monstermann/unplugin-tree-shake-import-namespaces",
    "type": "module",
    "version": "0.3.0",
    "description": "Tree-shake import namespaces with oxc.",
    "author": "Michael Ostermann <michaelostermann@me.com>",
    "license": "MIT",
    "homepage": "https://MichaelOstermann.github.io/tree-shake-import-namespaces",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MichaelOstermann/tree-shake-import-namespaces.git"
    },
    "bugs": {
        "url": "https://github.com/MichaelOstermann/tree-shake-import-namespaces/issues"
    },
    "keywords": [],
    "sideEffects": false,
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./esbuild": {
            "import": {
                "types": "./dist/esbuild.d.mts",
                "default": "./dist/esbuild.mjs"
            },
            "require": {
                "types": "./dist/esbuild.d.cts",
                "default": "./dist/esbuild.cjs"
            }
        },
        "./rolldown": {
            "import": {
                "types": "./dist/rolldown.d.mts",
                "default": "./dist/rolldown.mjs"
            },
            "require": {
                "types": "./dist/rolldown.d.cts",
                "default": "./dist/rolldown.cjs"
            }
        },
        "./rollup": {
            "import": {
                "types": "./dist/rollup.d.mts",
                "default": "./dist/rollup.mjs"
            },
            "require": {
                "types": "./dist/rollup.d.cts",
                "default": "./dist/rollup.cjs"
            }
        },
        "./rspack": {
            "import": {
                "types": "./dist/rspack.d.mts",
                "default": "./dist/rspack.mjs"
            },
            "require": {
                "types": "./dist/rspack.d.cts",
                "default": "./dist/rspack.cjs"
            }
        },
        "./types": {
            "import": {
                "types": "./dist/types.d.mts",
                "default": "./dist/types.mjs"
            },
            "require": {
                "types": "./dist/types.d.cts",
                "default": "./dist/types.cjs"
            }
        },
        "./vite": {
            "import": {
                "types": "./dist/vite.d.mts",
                "default": "./dist/vite.mjs"
            },
            "require": {
                "types": "./dist/vite.d.cts",
                "default": "./dist/vite.cjs"
            }
        },
        "./webpack": {
            "import": {
                "types": "./dist/webpack.d.mts",
                "default": "./dist/webpack.mjs"
            },
            "require": {
                "types": "./dist/webpack.d.cts",
                "default": "./dist/webpack.cjs"
            }
        }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./*"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "bundle": "tsdown",
        "release": "bun publish --access public"
    },
    "dependencies": {
        "@monstermann/tree-shake-import-namespaces": "workspace:*",
        "unplugin": "^3.0.0",
        "unplugin-utils": "^0.3.0"
    }
}
